   "source": [
    "from flask import Flask\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route(\"/\")\n",
    "def hello():\n",
    "    return \"Hello World!\"\n",
    "\n",
    "@app.route(\"/hello/<user>\")\n",
    "def greet(user):\n",
    "    return \"Hello, {0}\".format(user)"
   ]
